# import file
include ~/.config/i3/keybinding.conf
include ~/.config/i3/autostart.conf
include ~/.config/i3/window.conf

# main mod
set $mod Mod4
set $alt Mod1

# font 
font pango:JetBrainsMono NF 12

# set workspace output
workspace 1 output HDMI-1
workspace 2 output HDMI-1
workspace 3 output HDMI-1
workspace 4 output HDMI-1
workspace 5 output HDMI-1

# set workspace name
set $ws1 "1"
set $ws2 "2"
set $ws3 "3"
set $ws4 "4"
set $ws5 "5"
set $ws6 "6"
set $ws7 "7"
set $ws8 "8"
set $ws9 "9"
set $ws10 "10"

floating_modifier $mod
tiling_drag modifier titlebar
